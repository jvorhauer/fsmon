{
	"name": "fsmon",
  "module": "parcl.ts",
  "type": "module",
  "version": "1.1.0",
  "devDependencies": {
    "@types/bun": "latest",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@parcel/watcher": "^2.5.1",
    "date-fns": "^4.1.0"
  },
  "scripts": {
    "start": "bun parcl.ts",
    "dev": "bun --watch parcl.ts",
    "check-types": "tsc --noEmit --project tsconfig.json",
    "build": "bun build ./parcl.ts --compile --outfile fsmon"
  },
  "trustedDependencies": [
    "@parcel/watcher",
    "typescript",
    "date-fns"
  ]
}
